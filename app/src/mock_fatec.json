{
  "_id": "id",
  "options": {
    "project": "fatec",
    "logoUrl": "/fatec.jpeg",
    "title": "FATEC SJC - Matrícula simples",
    "successMessage": "Sucesso! Cadastro realizado."
  },
  "steps": [{
      "name": "Informaçōes de login",
      "items": [{
          "id": "nome",
          "label": "Nome",
          "rules": {
            "required": true,
            "regex": "^[a-zA-Z ]+$"
          }
        },
        {
          "id": "email",
          "label": "Email",
          "type": "email",
          "rules": {
            "required": true,
            "email": true
          }
        },
        {
          "id": "senha",
          "label": "Senha",
          "type": "password",
          "rules": {
            "required": true
          }
        }
      ]
    },
    {
      "name": "Informações pessoais",
      "items": [{
          "id": "genero",
          "label": "Gênero",
          "type": "radio",
          "rules": {
            "required": true
          },
          "options": [{
              "value": "M",
              "label": "Masculino"
            },
            {
              "value": "F",
              "label": "Feminino"
            }
          ]
        },
        {
          "id": "nascimento",
          "label": "Data de nascimento",
          "rules": {
            "required": true,
            "date_format": "DD/MM/YYYY"
          },
          "mask": "99/99/9999"
        },
        {
          "id": "rg",
          "label": "RG",
          "rules": {
            "required": true
          },
          "mask": "99.999.999-9"
        },
        {
          "id": "cpf",
          "label": "CPF",
          "rules": {
            "required": true
          },
          "mask": "999.999.999-99"
        },
        {
          "id": "mae",
          "label": "Nome Mãe",
          "rules": {
            "required": true,
            "regex": "^[a-zA-Z ]+$"
          }
        },
        {
          "id": "pai",
          "label": "Nome Pai (opcional)",
          "rules": {
            "required": false,
            "regex": "^[a-zA-Z ]+$"
          }
        }
      ]
    },
    {
      "name": "Informações residenciais",
      "items": [{
          "id": "celular",
          "label": "Celular",
          "rules": {
            "required": true,
            "min": 14
          },
          "mask": "(99)99999-9999"
        },
        {
          "id": "cep",
          "label": "CEP",
          "rules": {
            "required": true
          },
          "mask": "99999-999"
        }
      ]
    },
    {
      "name": "Informações essenciais",
      "items": [{
          "id": "formacao",
          "label": "Formaçāo",
          "type": "select",
          "rules": {
            "required": true
          },
          "options": [{
              "value": "Ensino médio completo",
              "label": "Ensino médio completo"
            },
            {
              "value": "Ensino superior incompleto",
              "label": "Ensino superior incompleto"
            },
            {
              "value": "Ensino superior completo",
              "label": "Ensino superior completo"
            }
          ],
          "defaultOption": "Ensino médio completo"
        },
        {
          "id": "geral",
          "label": "ENEM - Nota geral",
          "type": "select",
          "rules": {
            "required": true
          },
          "options": [{
              "value": "zero",
              "label": "0"
            },
            {
              "value": "baixo",
              "label": "0-400"
            },
            {
              "value": "media",
              "label": "400-700"
            },
            {
              "value": "alta",
              "label": "700-900"
            },
            {
              "value": "maxima",
              "label": "1000"
            }
          ]
        },
        {
          "id": "redacao",
          "label": "ENEM - Nota redação",
          "type": "select",
          "rules": {
            "required": true
          },
          "options": [{
              "value": "zero",
              "label": "0"
            },
            {
              "value": "baixo",
              "label": "0-400"
            },
            {
              "value": "media",
              "label": "400-700"
            },
            {
              "value": "alta",
              "label": "700-900"
            },
            {
              "value": "maxima",
              "label": "1000"
            }
          ]
        }
      ]
    }
  ]
}
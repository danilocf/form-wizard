{
  "_id": "id",
  "options": {
    "logoUrl": "/fatec.jpeg",
    "title": "FATEC SJC - Matrícula simples"
  },
  "steps": [
    {
      "name": "Informaçōes de login",
      "items": [
        {
          "id": "nome",
          "label": "Nome",
          "rules": {
            "required": true,
            "regex": "^[a-zA-Z ]+$"
          }
        },
        {
          "id": "email",
          "label": "Email",
          "type": "email",
          "rules": {
            "required": true,
            "email": true
          }
        },
        {
          "id": "senha",
          "label": "Senha",
          "type": "password",
          "rules": {
            "required": true
          }
        },
        {
          "id": "genero",
          "label": "Gênero",
          "type": "radio",
          "rules": {
            "required": true
          },
          "options": [
            { "value": "M", "label": "Masculino" },
            { "value": "F", "label": "Feminino" }
          ]
        },
        {
          "id": "formacao",
          "label": "Formaçāo",
          "type": "select",
          "rules": {
            "required": true
          },
          "options": [
            { "value": "Ensino médio completo", "label": "Ensino médio completo" },
            { "value": "Ensino superior incompleto", "label": "Ensino superior incompleto" },
            { "value": "Ensino superior completo", "label": "Ensino superior completo" }
          ],
          "defaultOption": "Ensino médio completo"
        }
      ]
    },
    {
      "name": "Outras",
      "items": [
        {
          "id": "cpf",
          "label": "CPF",
          "rules": {
            "required": true
          },
          "mask": "999.999.999-99"
        },
        {
          "id": "celular",
          "label": "Celular",
          "rules": {
            "required": true,
            "min": 14
          },
          "mask": "(99)99999-9999"
        }
      ]
    }
  ]
}